{
    "name": "jano/jano-ticketing",
    "description": "Jano Ticketing System",
    "keywords": ["php", "mysql", "redis", "event-ticketing"],
    "license": "GPL-3.0+",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1.0",
        "yajra/laravel-datatables-oracle": "^7.2",
        "namshi/jose": "^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "league/csv": "^8.0",
        "dingo/api": "1.0.*@beta",
        "tymon/jwt-auth": "1.0.*@beta",
        "ramsey/uuid": "^3.6",
        "sentry/sentry-laravel": "^0.6",
        "propaganistas/laravel-phone": "^2.8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Jano\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
