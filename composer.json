{
    "name": "jano-may-ball/ticketing",
    "description": "Jano Ticketing System",
    "keywords": ["php", "mysql", "event-ticketing"],
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Andrew Ying",
            "email": "hi@andrewying.com"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "arrilot/laravel-widgets": "^3.12",
        "barryvdh/laravel-dompdf": "^0.8",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "~1.4",
        "guzzlehttp/guzzle": "^6.2",
        "hashids/hashids": "^3.0",
        "jano-may-ball/settings": "^1.0",
        "kris/laravel-form-builder": "^1.15",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^6.0",
        "laravel/socialite": "^3.1",
        "laravel/tinker": "^1.0",
        "league/csv": "^9.1",
        "milon/barcode": "^5.3",
        "nwidart/laravel-modules": "^3.0",
        "owen-it/laravel-auditing": "^8.0",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.0",
        "spatie/laravel-menu": "^3.1",
        "valorin/pwned-validator": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/php-code-coverage": "^6.0",
        "phpunit/phpunit": "^7.0",
        "sebastian/phpcpd": "^4.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "exclude-from-classmap": [
            "modules/*/vendor/**"
        ],
        "psr-4": {
            "Jano\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "class": "Jano\\Composer\\InstallerRepository",
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"file_exists('storage/settings.hjson.example') || copy('storage/settings.hjson.example', 'storage/settings.hjson');\"",
            "@php -r \"getenv('APP_KEY') || shell_exec('php jano key:generate');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php jano package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "classmap-authoritative": true
    }
}
